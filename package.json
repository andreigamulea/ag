{
  "name": "app",
  "private": true,
  "dependencies": {
    "@hotwired/stimulus": "^3.2.2",
    "@hotwired/turbo-rails": "^8.0.13",
    "tailwindcss": "3.4.14",
    "caniuse-lite": "^1.0.30001712"
  },
  "devDependencies": {
    "esbuild": "^0.25.1"
  },
  "scripts": {
    "build": "esbuild app/javascript/*.* --bundle --outdir=app/assets/builds --minify",
